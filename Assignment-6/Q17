#include <stdio.h>

int main() {
    int num, i = 2, temp, sum = 0;

    scanf("%d", &num);

    if (num <= 1) {
        printf("Not Prime");
        return 0;
    }

    while (i <= num / 2) {
        if (num % i == 0) {
            printf("Not Prime");
            return 0;
        }
        i++;
    }

    temp = num;
    while (temp != 0) {
        sum = sum + (temp % 10);
        temp = temp / 10;
    }

    if (sum == 14) {
        printf("%d is Prime and sum is 14", num);
    } else {
        printf("%d is Prime but sum is not 14", num);
    }

    return 0;
}
