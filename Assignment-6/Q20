#include <stdio.h>

int main() {
    int num = 0, i, count = 0;

    while (num <= 9) {

        if (num > 1) {
            i = 2;
            while (i <= num / 2) {
                if (num % i == 0) {
                    break;
                }
                i++;
            }
            if (i > num / 2) {
                count++;
            }
        }

        num++;
    }

    printf("%d\n", count);
    return 0;
}
