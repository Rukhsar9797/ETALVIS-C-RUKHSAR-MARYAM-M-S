#include <stdio.h>

int main() {
    int n, num, count = 0;

    printf("Enter number: ");
    scanf("%d", &n);

    while (n >= 10) {  // loop until at least 2 digits left
        num = n % 100;  // last 2 digits

        if (num % 2 != 0) {
            count++;     // increment if odd
        }

        n = n / 10;      // remove last digit to slide window
    }

    printf("Total two-digit odd numbers: %d", count);

    return 0;
}
